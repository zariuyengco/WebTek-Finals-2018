<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="style/icons/car.png">

    <title>Service Provider</title>

    <!-- Bootstrap core CSS -->
    <link href="style/bootstrap.min.css" rel="stylesheet">
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">

    <!-- Custom styles for this template -->
    <link href="style/product.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:700,900" rel="stylesheet">
    <link href="blog.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  </head>

  <body>

   <% include partial/nav %>

        <div class="container">
          <div class="page-header">
            <h1>Welcome, CarGo Service Provider</h1>
          </div>

          <div class="col-md-5 toppad  pull-right col-md-offset-3">
            <a href="/edit-profile" class="btn btn-primary">Edit Profile</a>
            <a href="/logout" class="btn btn-default">Logout</a>
          </div><br><br>



          <div  class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad">
              <div class="panel panel-info">
            <div class="panel-heading">
              <h3 class="panel-title">Profile</h3>
            </div>
            <div class="panel-body">
              <div class="row">
                
                <div class=" col-md-9 col-lg-9 "> 
                  <table class="table table-user-information">
                    <tbody>
                      <tr>
                        <td>Firstname:</td>
                        <td><%= serviceUsers.firstName %></td>
                      </tr>
                      <tr>
                        <td>Lastname:</td>
                        <td><%= serviceUsers.lastName %></td>
                      </tr>
                      <tr>
                        <td>Contact Number:</td>
                        <td><%= serviceUsers.contactNo %></td>
                      </tr>
                      <tr>
                        <td>Email:</td>
                        <td><%= serviceUsers.email %></td> 
                      </tr>      
                    </tbody>
                  </table>
                </div>       
              </div>
            </div>
        </div>
      </div>
    </div>

   <% include partial/footer %>
  </body>
</html>
